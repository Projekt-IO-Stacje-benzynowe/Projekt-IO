<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>

<BorderPane
    fx:id="contentPane"
    xmlns="http://javafx.com/javafx"
    xmlns:fx="http://javafx.com/fxml"
    fx:controller="app.controllers.control_panel.logistics_coordinator_section.LogisticsMainController">
    <top>
        <HBox alignment="CENTER" spacing="20" >
            <Button mnemonicParsing="false" onAction="#goToViewRequests" prefHeight="35.0" prefWidth="140.0" text="View requests"/>

            <Button mnemonicParsing="false" onAction="#goToPlanDelivery" prefHeight="35.0" prefWidth="140.0" text="Plan a new delivery"/>

            <Button mnemonicParsing="false" onAction="#goToChooseDelivery" prefHeight="35.0" prefWidth="140.0" text="Choose a delivery"/>
        </HBox>
    </top>
    <left>
    </left>
    <center>
        <StackPane fx:id="tablePane">
        </StackPane>
    </center>
    <right>
    </right>
    <bottom>
    </bottom>
</BorderPane>
